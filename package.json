{
	"name": "contract",
	"version": "1.0.0",
	"scripts": {
		"openapi": "openapi",
		"spec:preview": "nodemon",
		"spec:build": "openapi b spec --source=src/docs/user.json",
		"spec:serve": "openapi s spec",
		"dtos:build": "openapi b typescript --dist=src/mock-proxy/dtos",
		"mock:server": "prism mock ./dist/index.json",
		"mock:proxy": "ts-node src/mock-proxy/index.ts",
		"mock:static": "npx ts-node src/static/index.ts"
	},
	"dependencies": {
		"@lubowiecki/node-openapi": "^1.0.0",
		"@lubowiecki/node-utility": "^1.0.0",
		"@stoplight/prism-cli": "^4.8.0",
		"express": "^4.17.3",
		"fs-extra": "^10.0.1",
		"http-proxy-middleware": "^2.0.3",
		"node-http-proxy-json": "^0.1.9",
		"nodemon": "^2.0.15",
		"ts-node": "^10.7.0"
	},
	"devDependencies": {
		"@lubowiecki/eslint-config-base": "^1.0.0",
		"@types/express": "^4.17.13",
		"@types/fs-extra": "^9.0.13",
		"@types/http-proxy-middleware": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"eslint": "^8.10.0",
		"eslint-plugin-import": "^2.25.4"
	}
}
